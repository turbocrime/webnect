{
	"compilerOptions": {
		"module": "es2022",
		"target": "es2022",
		"lib": [
			"dom",
			"es2022",
			"webworker"
		],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
